class Demo1
{
 int a;
 static int b;
static 
{
b=20;
System.out.println("static block");
System.out.println(b);
}
{
a=10;
System.out.println("non static block");
System.out.println(a);
}



public static void main(String args[])
{
Demo1 d1=new Demo1();
Demo1 d2=new Demo1();
System.out.println("main function");


}
}
