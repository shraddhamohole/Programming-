//C program to print transpose of matrix
#include<stdio.h>

void main()
{
	int a[10][10], i, j, row, col;
	printf("how many rows are you want?\n");
	scanf_s("%d", &row);
	printf("how many column are you want?\n");
	scanf_s("%d", &col);
	printf("enter the array elements\n");
	for (i = 0; i < row; i++)
	{
		for (j = 0; j < col; j++)
		{
			scanf_s("%d", &a[i][j]);
		}
	}
	printf("Array elements are\n");
	for (i = 0; i < row; i++)
	{
		for (j = 0; j < col; j++)
		{
			printf("%d\t", a[i][j]);
		}
		printf("\n");
	}
	printf("Transpose of Matrix: \n"); 
          for (j = 0; j < col; j++)
	{
		for (i = 0; i < row; i++)
		{
			printf("%d\t", a[i][j]);
		}
		printf("\n");
	}

}
