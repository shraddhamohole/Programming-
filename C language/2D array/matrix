//matrix

#include<stdio.h>
void main()
{
	int  a[100][100];
	int i, j, row, col;
	printf("how may row");
	scanf_s("%d", &row);
	printf("how may colom");
	scanf_s("%d", &col);
	for (i = 0; i < row; i++)
	{
		for (j = 0; j < row; j++)
		{
			printf("enter a[%d][%d]:",i,j);
			scanf_s("%d", &a[i][j]);
			

		}

	}
	printf("matrix is\n");
	for (i = 0; i < row; i++)
	{
		printf("\n");
		for (j = 0; j < row; j++)
		{
			printf("%d\t", a[i][j]);
		}
		printf("\n");

	}



}




