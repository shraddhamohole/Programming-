//C program to find unique element

#include<stdio.h>
void main()
{
	int i,n,a[100],count,j;
	printf("enter size of array");
	scanf_s("%d",&n);
	printf("enter array element");
	for(i=0;i<n;i++)
	{
		scanf_s("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		count=0;
	for(j=0;j<n;j++)
	{
		if(a[i]==a[j])
		{
			count++;
		}
	}
		
	
	if(count==1)
	{
		printf("\n%d is unique",a[i]);
		
	}
	
	}
	
}

