//C program to find maximum and minimun no in array
#include<stdio.h>
void main()
{
	int n,i,max,a[100],min;
	printf("enter  size of array\n");
	scanf_s("%d",&n);
	printf("enetr element of array\n");
	for(i=0;i<n;i++)
	{
		scanf_s("%d",&a[i]);
	}
	max=a[0];
	for(i=0;i<n;i++)
	{
		if(max<a[i])
			max=a[i];
	}
	min=a[0];
	for(i=0;i<n;i++)
	{
		if(min>a[i])
			min=a[i];
	}
	printf("\nmax - %d",max);
	printf("\nmin - %d",min);

}
	
