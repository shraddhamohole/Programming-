//C program to copy one array element into other

#include<stdio.h>
void main()
{
	int a[100],n,i,b[100];
	printf("enter array size");
	scanf_s("%d",&n);
	printf("enter array elements");
	for(i=0;i<n;i++)
	{
		scanf_s("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		b[i]=a[i];
	}
	printf("cpy array");
	for(i=0;i<n;i++)
	{
		printf("\n%d",b[i]);
	}
}



