// c program to store square of element in same array
#include<stdio.h>
void main()
{
	int a[100],b[100],i,n;
	printf("size of array");
	scanf_s("%d",&n);
	printf("enter array elements\n");
	for(i=0;i<n;i++)
	{
		scanf_s("%d",&a[i]);
	}
	printf("store square of element in second array\n");
	for(i=0;i<n;i++)
	{
		b[i]=a[i]*a[i];
		printf("%d\n",b[i]);
	}
}
