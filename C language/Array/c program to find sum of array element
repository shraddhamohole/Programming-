//c program to find sum of array element
#include<stdio.h>
void main()
{
	int n,sum,i, a[100],j;
	printf("enter size of array\n");
	scanf_s("%d",&n);
	printf("enter array element\n");
	for(i=0;i<n;i++)
	{
		scanf_s("%d",&a[i]);
	}
	sum=0;
	for(i=0;i<n;i++)
	{
		sum=sum+a[i];


	}
	printf("\nsum of array element is %d\n",sum);
}
