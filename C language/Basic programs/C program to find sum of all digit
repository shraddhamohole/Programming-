//C program to find sum of all digit
#include<stdio.h>
#include<conio.h>
void  main()
{
    int no,sum=0,i;
    printf("Enter any number: ");
    scanf_s("%d", &no);
    while(no>0)
    {
		
        i = no%10;
        sum = sum + i;
         no=no/10;
        
    }
    printf("\nSum of Digit = %d", sum);
    _getch();
}
