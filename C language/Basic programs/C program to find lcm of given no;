//C program to find lcm of given no
#include<conio.h>
{
	int n1,n2,max;
	printf("enter two nos\n");
	scanf_s("%d%d",&n1,&n2);
	
		if(n1>n2)
		{
			max=n1;
		}
		else
		{
			max=n2;
		}
		while(1)
		{
		
			if(max%n1==0 && max%n2==0)
			{
				printf("lcm is %d", max);
			break;
			}
		++max;
		}
		_getch();
}
