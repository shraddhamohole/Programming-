//c program to print sum of all prime no between 1 to n
#include<stdio.h>
void main()
{
	int n,i,j,count,sum=0;
	printf("enter any no");
	scanf_s("%d",&n);
	i=1;
	while(i<=n)
	{
		count=0;
		i++;
	for(j=1;j<=n;j++)
	{
		
		if(i%j==0)
		{

			count++;
		}
	}
		
		if(count==2)
		
	{
		sum=sum+i;
	}
	}
printf("sum of prime no from 1 to %d =%d\n",n,sum);
     
	
	
	
	
}



